Info3: <<
Package: spatialite-gui
Version: 1.7.1
Revision: 2
Description: User friendly GUI tool to SpatiaLite
License: GPL
Homepage: http://www.gaia-gis.it/spatialite
Maintainer: BABA Yoshihiko <babayoshihiko@mac.com>
DescUsage: type `spatialite-gui' in Terminal

Depends: <<
  libgaiagraphics1-shlibs,
  libgeos3.6.1-shlibs,
  libgettext8-shlibs,
  librasterlite2-shlibs,
  libspatialite7-shlibs,
  libxml2-shlibs (>= 2.9.1-1),
  wxcocoa295-shlibs
<<
BuildDepends: <<
  fink-package-precedence,
  libgaiagraphics1,
  libgeos3.6.1,
  libgettext8-dev,
  librasterlite2,
  libspatialite7,
  libxml2 (>= 2.9.1-1),
  pkgconfig,
  wxcocoa295
<<
GCC: 4.0

# Unpack Phase.
Source: http://www.gaia-gis.it/gaia-sins/spatialite_gui-%v.tar.gz
Source-MD5: c917f40810607784528b4db58cd36efb

# Debian spatialite-gui1.7.1-5:
#   03-link-sqlite3.patch
#   04-wx3.0-compat.patch
#   05-setlocale.patch
# Actually pass the geos -I flags after detecting them
# Fix some spaceing bugs in $LIBS
# Local -I before global
PatchFile: %n.patch
PatchFile-MD5: 67688a720470f2106c236940ae237be9

ConfigureParams: --with-geosconfig=%p/opt/libgeos3.6.1/bin/geos-config

CompileScript: <<
#!/bin/sh -xe
  ./configure %c
  make
  # make -f Makefile-static-MacOsX
  fink-package-precedence .
<<

# Install Phase.
InstallScript: <<
#!/bin/sh -xe
  make install prefix=%i
<<
DocFiles: COPYING

# End of Info3
<<
